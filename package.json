{
  "name": "scrum-dao",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf web-app",
    "copy-data": "cpx \"src/**/*.ini\" ./web-app",
    "migrate": "truffle migrate --network development",
    "prebuild-app": "npm run clean",
    "build-app": "ng build",
    "prebuild-server": "npm run clean",
    "build-server": "tsc -p src/server/tsconfig.json",
    "postbuild-server": "npm run copy-data",
    "predev-start-angular": "npm run migrate && node ./populateContracts.js",
    "dev-start-angular": "ng serve --proxy-config proxy.config.json --watch",
    "predev-start-server": "npm run build-server",
    "dev-start-server": "export NODE_ENV=development && export LOG_LEVEL=debug && node web-app/server/server.js",
    "dev-start": "concurrently --kill-others-on-fail \"npm run dev-start-angular\" \"npm run dev-start-server\"",
    "prestart": "npm run clean && npm run build-app && npm run build-server",
    "start": "export NODE_ENV=production && node web-app/server/server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "contributors": [
    "Pavel Shalamkov"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.6",
    "@angular/cdk": "^2.0.0-beta.7",
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/flex-layout": "^2.0.0-beta.7",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "@types/async": "^2.0.44",
    "@types/express": "^4.0.38",
    "@types/kue": "^0.11.8",
    "@types/winston": "^2.3.7",
    "amdefine": "^1.0.1",
    "async": "^2.5.0",
    "body-parser": "^1.17.2",
    "consolidate": "^0.14.5",
    "core-js": "^2.5.0",
    "cpx": "^1.5.0",
    "ejs": "^2.5.7",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.2",
    "express": "^4.15.4",
    "filename-regex": "^2.0.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
