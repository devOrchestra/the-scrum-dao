    "request": "^2.83.0",
    "roboto-fontface": "^0.8.0",
    "rxjs": "^5.4.3",
    "serve-favicon": "^2.4.3",
    "truffle": "^3.4.11",
    "truffle-contract": "^1.1.11",
    "types-ethereumjs-tx": "bitbucket:devorchestra/types-ethereumjs-tx",
    "web3": "^0.18.2",
    "winston": "^2.4.0",
    "zeppelin-solidity": "^1.3.0",
    "zone.js": "^0.8.17"
